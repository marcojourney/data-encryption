{
  "name": "psr-container",
  "version": "1.1.0",
  "description": "Common Container Interface (PHP FIG PSR-11).",
  "keywords": [
    "psr",
    "psr-11",
    "container",
    "container-interop",
    "container-interface"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyjs/psr-container",
  "scripts": {
    "cs-fix": "prettier --write src",
    "cs": "prettier --check src",
    "build": "tsc",
    "prepare": "rm -Rf dist && npm run build && npm run cs"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "prettier": "2.1.2",
    "typescript": "^4.0.5"
  }
}
